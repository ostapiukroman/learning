{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true,
    "node": true,
  },
  "rules": {
    "array-bracket-spacing": 1,
    "eol-last": 2,
    "block-spacing": [1, "never"],
    "brace-style": 2,
    "camelcase": 2,
    "comma-dangle": [2, "always-multiline"],
    "quotes": [1, "single"],
    "wrap-regex": 2,
    "no-var": 2,
    "arrow-spacing": 1,
    "no-floating-decimal": 2,
    "no-eval": 2,
    "accessor-pairs": 2,
    "eqeqeq": 2,
    "no-console": 1,
    "no-debugger": 2
  }
}
